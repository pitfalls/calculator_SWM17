            android:minWidth="50dip"
            android:text="9"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp" />

        <Button
            android:id="@+id/Button_ID_Gleich"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_alignParentBottom="true"
            android:layout_toStartOf="@+id/Button_ID_Mal"
            android:minWidth="50dip"
            android:text="="
            android:textSize="18sp"
            tools:layout_editor_absoluteX="160dp"
            tools:layout_editor_absoluteY="296dp" />

        <Button
            android:id="@+id/Button_ID_Plus"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_alignBottom="@+id/button9"
            android:layout_alignTop="@+id/button9"
            android:layout_toEndOf="@+id/Button_ID_3"
            android:minWidth="50dip"
            android:text="+"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="291dp"
            tools:layout_editor_absoluteY="0dp" />

        <Button
            android:id="@+id/Button_ID_4"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:minWidth="50dip"
            android:text="4"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="160dp"
            tools:layout_editor_absoluteY="89dp" />

        <Button
            android:id="@+id/Button_ID_6"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_alignBottom="@+id/Button_ID_5"
            android:layout_alignTop="@+id/Button_ID_5"
            android:layout_toEndOf="@+id/Button_ID_2"
            android:minWidth="50dip"
